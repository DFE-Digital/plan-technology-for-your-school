@model Dfe.PlanTech.Web.Models.CookiesViewModel

@section BeforeContent {
    <partial name="BackButton" />
}

<partial name="CookiesPreferencesSetBanner" model="@Model"/>

<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
        <partial name="Components/PageComponentFactory" model="@Model.Title"/>

        @if (Model.Content != null)
        {
            foreach (var content in Model.Content)
            {
                <partial name="Components/PageComponentFactory" model="content"/>
            }
        }

        <partial name="CookiePreferencesForm" model="@Model.Cookie"/>
    </div>
</div>